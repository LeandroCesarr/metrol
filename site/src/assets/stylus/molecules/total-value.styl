.m-total-value {
  width: 100%;
  padding: 10px 24px;
  display: flex;
  margin 20px 0;
  justify-content: space-between;
  background-color: #fff;
  box-shadow: 0 5px 5px -3px rgba(0,0,0,.2), 0 8px 10px 1px rgba(0,0,0,.14), 0 3px 14px 2px rgba(0,0,0,.12);

  p {
    margin: 0;
    font-size: 14px;
    color: rgba(0,0,0,.87);
  }
}